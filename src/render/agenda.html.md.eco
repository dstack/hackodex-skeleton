---
title: "The Agenda"
layout: "text-page"
---

<h3>All times are in <%= @tzOnly() %></h3>

<% for track of @agenda: %>
<%= if track != 'def' then 'track' else '' %>
<div class="agenda-track">
<% for day of @agenda[track]: %><div class="day-divider"><span><%= day %></span></div><% for agendaItem, indx in@agenda[track][day]: %><%- @partial('agendaItem', {agendaItem: agendaItem, indx: indx}) %><% end %>
<% end %>
</div>
<% end %>
