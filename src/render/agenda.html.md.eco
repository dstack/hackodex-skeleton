<div class="agenda-track">


<% for agendaItem in @agenda: %>

<div class="agenda-marker"></div>
<div class="agenda-item">
  <div class="date-time"><%= @agendaDateFormat(agendaItem.start) %> - <%= @agendaDateFormat(agendaItem.end) %></div>
  <b><%= agendaItem.eventName %></b><br/><br/>
  <p><%= agendaItem.description %></p>
</div>

